{
    "env": {
        "node": true,
        "jest": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "semi": ["off"],
      "comma-dangle": ["off"],
      "import/prefer-default-export": ["off"],
      "no-underscore-dangle": ["off"],
      "no-plusplus": ["off"],
      "camelcase": ["off"]
    }
}